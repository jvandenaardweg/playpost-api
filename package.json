    "@types/response-time": "^2.3.3",
    "@types/url-parse": "^1.4.3",
    "@types/uuid": "^3.4.5",
    "@types/ws": "^6.0.1",
    "app-root-path": "^2.2.1",
    "auto-changelog": "^1.14.1",
    "aws-sdk": "^2.489.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "class-validator": "^0.9.1",
    "compression": "^1.7.4",
    "date-fns": "^1.30.1",
    "dotenv": "^8.0.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-brute": "^1.0.1",
    "express-brute-cloudflare": "^1.0.4",
    "express-brute-redis": "0.0.1",
    "express-cluster": "0.0.5",
    "express-rate-limit": "^5.0.0",
    "express-validator": "^6.1.1",
    "fluent-ffmpeg": "^2.1.2",
    "forever": "^1.0.0",
    "fs-extra": "^8.1.0",
    "helmet": "^3.18.0",
    "husky": "^3.0.0",
    "in-app-purchase": "^1.11.4",
    "ioredis": "^4.11.1",
    "jest": "^24.8.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "locale-code": "^2.0.2",
    "md5": "^2.2.1",
    "music-metadata": "^4.2.0",
    "node-fetch": "^2.6.0",
    "normalize-url": "^4.3.0",
    "npm-check": "^5.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.11.0",
    "pino": "^5.12.6",
    "pino-pretty": "^3.2.0",
    "polly-ssml-split": "^0.1.0",
    "rate-limit-redis": "^1.6.0",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "typeorm": "^0.2.18",
    "typescript": "^3.5.3",
    "typescript-tslint-plugin": "^0.5.3",
    "url-parse": "^1.4.7",
    "uuid": "^3.3.2"
  }
}
