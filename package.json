  "main": "dist/index.js",
  "engines": {
    "node": "10.15.0",
    "npm": "6.5.0"
  },
  "scripts": {
    "dev": "npm run dev:database && npm run dev:nodemon",
    "dev:nodemon": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' --transpile-only src/index.ts",
    "dev:database": "docker-compose up -d",
    "dev:docker:rebuild": "docker-compose up -d --force-recreate --build",
    "build": "npm run build:typescript:production",
    "build:typescript": "tsc",
    "build:typescript:production": "tsc -p tsconfig.production.json",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "./node_modules/eslint/bin/eslint.js *.js --ignore-path .gitignore",
    "eslint:fix": "./node_modules/eslint/bin/eslint.js *.js --ignore-path .gitignore --fix",
    "prisma": "prisma",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jvandenaardweg/readto-api.git"
  },
  "author": "Jordy van den Aardweg",
