    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.10.0",
    "pino": "^5.12.3",
    "pino-pretty": "^3.0.0",
    "polly-ssml-split": "^0.1.0",
    "rate-limit-redis": "^1.6.0",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "ts-loader": "^5.4.6",
    "typeorm": "^0.2.17",
    "typescript-tslint-plugin": "^0.3.1",
    "url-parse": "^1.4.7",
    "uuid": "^3.3.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
