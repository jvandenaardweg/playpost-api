{
  "name": "playpost-api",
  "version": "1.0.1",
  "description": "",
  "main": "dist/index.js",
  "engines": {
    "node": "^10.15.0",
    "npm": "^6.8.0"
  },
  "scripts": {
    "start": "forever -m 5 dist/index.js",
    "dev": "npm run dev:database && npm run dev:worker & npm run dev:index",
    "debug": "npm run dev:database && npm run debug:worker & npm run debug:index",
    "debug:index": "ts-node-dev --inspect=9230 --respawn --transpileOnly ./src/index.ts",
    "debug:worker": "ts-node-dev --inspect=9231 --respawn --transpileOnly ./src/worker.ts",
    "dev:index": "ts-node-dev --respawn --transpileOnly src/index.ts --ignore-watch src/worker.ts",
    "dev:worker": "ts-node-dev --respawn --transpileOnly src/worker.ts --ignore-watch src/index.ts &",
    "dev:database": "docker-compose up -d",
    "dev:docker:rebuild": "docker-compose up -d --force-recreate --build",
    "database:sync": "ts-node-dev src/database-sync.ts",
    "build": "npm run build:typescript:production",
    "heroku-postbuild": "npm run build",
    "build:webpack": "webpack",
    "build:typescript": "npx tsc",
    "build:typescript:production": "npx tsc -p tsconfig.production.json",
    "caches:empty": "ts-node-dev src/empty-cache.ts",
    "worker": "forever -m 5 dist/worker.js",
    "test": "NODE_ENV=unittest jest --silent --silent",
    "test:circleci": "NODE_ENV=unittest jest --silent --silent --maxWorkers=2",
    "pretest": "NODE_ENV=unittest npm run eslint",
    "test:watch": "NODE_ENV=unittest jest --watch --silent --coverage",
    "test:integration:local": "NODE_ENV=integrationtest BASE_API_URL=http://localhost:3000 jest --roots=./tests --runInBand --silent",
    "test:integration:staging": "NODE_ENV=integrationtest BASE_API_URL=https://api-staging.playpost.app jest --roots=./tests --runInBand --silent",
    "test:integration:production": "NODE_ENV=integrationtest BASE_API_URL=https://api.playpost.app jest --roots=./tests --runInBand --silent",
    "eslint": "./node_modules/eslint/bin/eslint.js *.js --ignore-path .gitignore",
    "eslint:fix": "./node_modules/eslint/bin/eslint.js *.js --ignore-path .gitignore --fix",
    "typeorm:schema:sync": "typeorm schema:sync",
    "typeorm:migration:revert": "typeorm migration:revert",
    "typeorm:migration:run": "typeorm migration:run",
    "typeorm:cache:clear": "typeorm cache:clear",
    "upgrade-interactive": "npm-check --update",
    "typeorm:local:schema:sync": "ts-node ./node_modules/.bin/typeorm schema:sync",
    "tsc:watch": "tsc -p ./ --noEmit -w",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jvandenaardweg/playpost-api.git"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "unreleased": true,
    "commitLimit": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  },
  "author": "Jordy van den Aardweg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jvandenaardweg/playpost-api/issues"
  },
  "homepage": "https://github.com/jvandenaardweg/playpost-api#readme",
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.0.19",
    "@google-cloud/pubsub": "^0.30.1",
    "@google-cloud/storage": "^2.5.0",
    "@google-cloud/text-to-speech": "^1.1.2",
    "@sentry/cli": "^1.46.0",
    "@sentry/integrations": "^5.5.0",
    "@sentry/node": "^5.5.0",
    "@types/app-root-path": "^1.2.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "0.0.36",
    "@types/express": "^4.17.0",
    "@types/express-brute": "0.0.37",
    "@types/express-brute-redis": "0.0.1",
    "@types/express-cluster": "0.0.0",
    "@types/express-rate-limit": "^3.3.0",
    "@types/fluent-ffmpeg": "^2.1.10",
    "@types/fs-extra": "^8.0.0",
    "@types/helmet": "0.0.43",
    "@types/in-app-purchase": "^1.10.0",
    "@types/ioredis": "^4.0.13",
    "@types/jest": "^24.0.15",
    "@types/joi": "^14.3.3",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/md5": "^2.1.33",
    "@types/node": "^12.6.1",
    "@types/node-fetch": "^2.3.7",
    "@types/passport": "^1.0.0",
