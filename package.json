    "compression": "^1.7.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-async-errors": "^3.1.1",
    "franc": "^4.0.0",
    "fs-extra": "^7.0.1",
    "helmet": "^3.15.1",
    "jsdom": "^13.2.0",
    "jsonwebtoken": "^8.4.0",
    "jssoup": "0.0.10",
    "mp3-duration": "^1.1.0",
    "node-fetch": "^2.3.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "polly-ssml-split": "^0.1.0",
