    "@sentry/cli": "^1.41.2",
    "@sentry/integrations": "^5.1.2",
    "@sentry/node": "^5.1.2",
    "@types/pino": "^5.8.7",
    "@types/url-parse": "^1.4.3",
    "app-root-path": "^2.2.1",
    "aws-sdk": "^2.448.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "class-validator": "^0.9.1",
    "compression": "^1.7.4",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-async-errors": "^3.1.1",
    "express-brute": "^1.0.1",
    "express-brute-redis": "0.0.1",
    "express-rate-limit": "^3.5.0",
    "express-validator": "^5.3.1",
    "fluent-ffmpeg": "^2.1.2",
    "forever": "^1.0.0",
    "fs-extra": "^7.0.1",
    "helmet": "^3.16.0",
    "ioredis": "^4.9.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "locale-code": "^2.0.2",
    "md5": "^2.2.1",
    "music-metadata": "^3.7.0",
    "node-fetch": "^2.5.0",
