    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^24.7.1",
    "npm-check": "^5.9.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "ts-node-dev": "^1.0.0-pre.35",
    "tsc-watch": "^2.1.2",
    "tslint": "^5.16.0",
